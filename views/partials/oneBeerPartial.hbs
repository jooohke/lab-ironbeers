<div id="one-beer-div" class="beer-card">
    
    <div>
    <img src="{{this.image_url}}">
    </div>
    

    <div>
    <h3>{{this.name}}</h3>
    <p><i>{{this.tagline}}</i></p>
    <p>{{this.description}}</p>

    <ul>{{#each this.food_pairing}}
    <li>{{this}}</li>
    {{/each}}</ul>

    <p>{{this.brewers_tips}}</p>
    </div>
</div>